{
	"manifest_version": 2,
	"version": "0.1.1.3",
	
	"name": "Dota2Ru-smiles",
	"description": "Использование сторонних смайлов для форума и новый чат",
	"content_scripts": [ {
		"css": [ "extension.css" ],
		"js": [ "extension.js" ],
		"matches": [ "*://dota2.ru/forum*" ],
		"run_at": "document_end"
	} ],
	"icons": {
		"128": "/assets/favicon_128.png",
		"16": "/assets/favicon_16.png",
		"48": "/assets/favicon_48.png"
	},
	"web_accessible_resources": [ "/assets/*" ]
}