{
	"manifest_version": 2,
	"version": "0.0.5.1",
	
	"name": "Dota2Ru",
	"description": "Использование сторонних смайлов для форума",
	"content_scripts": [ {
		"css": [ "extension.css" ],
		"js": [ "extension.js" ],
		"matches": [ "*://dota2.ru/forum*" ]
	} ],
	"icons": {
		"128": "/assets/favicon_128.png",
		"16": "/assets/favicon_16.png",
		"48": "/assets/favicon_48.png"
	},
	"web_accessible_resources": [ "/assets/*" ]
}